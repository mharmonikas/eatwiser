{
  "name": "circle",
  "version": "1.0.0",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "watcher": "onchange '**/*.md' \"**/**/*.{js,css,scss,vue}\" -- prettier --write {{changed}}",
    "lint": "vue-cli-service lint",
    "lint:fix": "eslint ./src/**/*.{js,vue} --fix --color",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "prettier:format-all": "prettier --write \"**/*.{vue,scss,sass,js,json}\"",
    "prettier:check": "prettier \"**/*.{vue,scss,sass,js,json}\" --list-different",
    "firebase:deploy:ci": "firebase deploy --non-interactive --token $FIREBASE_TOKEN",
    "bundlesize": "bundlesize"
  },
  "engines": {
    "npm": ">=5.5.0",
    "node": ">=9.3.0"
  },
  "dependencies": {
    "compression-webpack-plugin": "^3.1.0",
    "core-js": "^3.6.5",
    "is_js": "^0.9.0",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "pwacompat": "^2.0.12",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.11",
    "vue-head": "^2.2.0",
    "vue-moment": "^4.1.0",
    "vue-router": "^3.1.6",
    "vue-social-sharing": "^3.0.0-beta.11",
    "vuetify": "^2.2.27",
    "vuex": "^3.3.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.150",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-e2e-cypress": "4.1.2",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-pwa": "^3.12.1",
    "@vue/cli-plugin-unit-jest": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-prettier": "^5.1.0",
    "@vue/test-utils": "^1.0.0-beta.33",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-lodash": "^3.3.4",
    "bundlesize": "github:kefranabg/bundlesize",
    "deepmerge": "^4.2.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^5.2.3",
    "fibers": "^4.0.3",
    "file-loader": "^6.0.0",
    "firebase-tools": "^7.16.2",
    "node-sass": "^4.14.1",
    "prerender-spa-plugin": "^3.4.0",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "size-plugin": "^2.0.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue-loader": "^15.9.1",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
